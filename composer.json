{
    "name": "everycheck/test-api-rest-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily test a rest api",
    "keywords": [
        "test",
        "api",
        "rest"
    ],
    "license": "MIT",
    "require": {
        "coduo/php-matcher": "^3.0",
        "jms/serializer-bundle": "^2.4",
        "myclabs/deep-copy": "^1.10",
        "phpunit/phpunit": "^6.0|^7.0",
        "symfony/phpunit-bridge": "^5.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/orm": "^2.6",
        "guzzlehttp/guzzle": "^6.3",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.5.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/symfony": "4.4.",
        "symfony/web-server-bundle": "^4.4",
        "twig/extensions": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "EveryCheck\\TestApiRestBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryCheck\\TestApiRestBundle\\Tests\\": "Tests"
        },
        "files": ["./Tests/sampleProject/app/AppKernel.php"]
    },
    "config": {
        "sort-packages":true
    }
}
